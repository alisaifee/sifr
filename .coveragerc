[run]
include = 
    sifr/*
omit =
    /*/sifr/_version*
    /*/sifr/backports/*

[report]
exclude_lines =
    pragma: no cover
    # Don't complain if non-runnable code isn't run:
    if __name__ == .__main__.:
    # ignore ImportErrors
    except ImportError
    # ignore not implemented features
    raise NotImplementedError
